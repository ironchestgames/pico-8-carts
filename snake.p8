-- apple and poop

-- apple
apples={}

apple={
	norm=1,
	fast=2,
	slow=3,
}

apple.new=function(typ)
	a={}
	a.x,a.y=getemptypos()
	a.rotten=false
	a.rottime=150
	a.typ=typ or apple.norm
	add(apples,a)
	return a
end

apple.nearany=function(x,y)
	for a in all(apples) do
		if abs(a.x-x)<=1 and
					abs(a.y-y)<=1 then
			return true
		end
	end
	return false
end

-- poop
poops={}

poop={}

poop.new=function(x,y)
	p={}
	p.x=x
	p.y=y
	add(poops,p)
	return p
end

