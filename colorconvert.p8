function ic_setdrawcolors(cstr)
	for i=1,#cstr do
		pal(i,
			tonum(
				'0x'..sub(cstr,i,i)))
	end
end

function _draw()
 	
 ic_setdrawcolors(col)
 cls(0)
 spr(0,32,32)
 spr(1,48,32)
 pal()
end
