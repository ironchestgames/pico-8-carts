pico-8 cartridge // http://www.pico-8.com
version 42
__lua__

function _init()
 summoner={
  x=2,y=7,
  update=function(_summoner)
   if btnp(2) then
    _summoner.y-=1
   end
   if btnp(3) then
    _summoner.y+=1
   end
  
   _summoner.y=mid(1,_summoner.y,14)
  end,
 }

 swordsgubb={
  x=1,y=3,
  update=function(_g)
   
  end,
 }

 gubbs={
  summoner,
 }
end

function _update60()
 for _g in all(gubbs) do
  _g.update(_g)
 end

end

function _draw()
 cls()

 camera(8,0)

 rectfill(8,0,136,7,1)
 rectfill(8,120,136,128,1)

 for _g in all(gubbs) do
  rectfill(_g.x*8,_g.y*8,_g.x*8+7,_g.y*8+7,8)
 end
end


