pico-8 cartridge // http://www.pico-8.com
version 39
__lua__
function tan(a)
 return sin(a)/cos(a)
end

function shearx(dx,dy,sx,sy,sw,sh,offset)
 for y=0,sh-1 do
  local t=2*y-sh
  sspr(sx,sy+y,sw,1,dx+t*offset-sw/2,dy+y-sh/2)
 end
end
function sheary(dx,dy,sx,sy,sw,sh,offset)
 for x=0,sw-1 do
  local t=2*x-sw
  sspr(sx+x,sy,1,sh,dx+x-sw/2,dy+t*offset-sh/2)
 end
end

local _angle=0
function _update()
 if btn(1) then
  _angle-=0.005
 elseif btn(0) then
  _angle+=0.005
 end
 -- if not btn(4) then
	--  _angle=sin(_t/256)/16
 -- 	_t+=1
	-- end
end

function _draw()
 cls()
 palt(0,false)
 spr(1,56,56,2,2)
 poke(0x5f54,0x60)
 shearx(64,64,0,32,128,64,-tan(_angle))
 sheary(64,64,32,0,64,128,sin(_angle))
 shearx(64,64,0,32,128,64,-tan(_angle))
 
 poke(0x5f54,0x0)
end
